<%-include("../includes/head.ejs")%>
  </head>
  <body>
    <div class="container">
     <%-include("../includes/navigation.ejs")%>
    </div>
    <div class="container ">
        <div class="row centered-form">
        <div class="col-xs-12 col-sm-8 col-md-4 col-sm-offset-2 col-md-offset-4 m-auto">
        	<div class="panel panel-default">
        		<div class="panel-heading">
			    		<h3 class="panel-title">Add Item </h3>
			 			</div>
			 			<div class="panel-body">
			    		<form  action ='/postaddProduct'role="form" method='POST'>
			    			<div class="form-group">
			            		<input type="text" name="title" id="title" class="form-control input-sm" placeholder="Product name">
			    			</div>			    			
							<div class="form-group">
			    				<input type="text" name="imageUrl1" id="imageUrl1" class="form-control input-sm" placeholder="Image Url">
							</div>
							<div class="form-group">
			    				<input type="text" name="imageUrl2" id="imageUrl" class="form-control input-sm" placeholder="Image Url">
							</div>
							<div class="form-group">
			    				<input type="text" name="category"  class="form-control input-sm" placeholder="Category">
							</div>
			    			<div class="form-group">
			    				<input type="number" name="price"  class="form-control input-sm" placeholder="Price">
							</div>
							<div class="form-group">
								<textarea class="form-control" name="Description" rows="5" id="comment" placeholder="Description"></textarea>
			    			</div>
			    			<input type="submit" value="Add" class="btn btn-info btn-block">
			    		</form>
			    	</div>
	    		</div>
    		</div>
    	</div>
    </div>
  
  <hr>
  <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  </body>
  </html>